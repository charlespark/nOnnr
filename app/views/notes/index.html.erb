<div class="center">
<h1>List of rnnOns</h1>

<table cellpadding="10">
  <thead>
    <tr>
      <th>Word</th>
      <th>Definition</th>
      <th>What You Wrote</th>
      <th>Published</th>
      <th>Actions</th>
      <th colspan="6"></th>
    </tr>
  </thead>

  <tbody>
    <% @notes.each do |note| %>
      <tr>
        <td><strong><%= note.word %></strong></td>
        <td><%= note.meaning %></td>
        <td><%= note.content %></td>
        <td><%= note.local_time %></td>
        <td><strong><%= link_to 'Show', note %></strong></td>
        <td><strong><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></strong></td>
<!--         <td><%#= link_to 'Edit', edit_note_path(note) %></td> -->

      </tr>
    <% end %>
  </tbody>
</table>

<br>
<strong><%= link_to 'New rnnOn', new_note_path %></strong>
</div>
<br>
